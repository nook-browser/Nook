  "primaryContentSections": [
    {
      "kind": "content",
      "content": [
        {
          "text": "Overview",
          "type": "heading",
          "anchor": "overview",
          "level": 2
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "text": "Use the WebKit framework to integrate richly styled web content into your app’s native content. WebKit offers a full browsing experience for your content, offering a platform-native view and supporting classes to:",
              "type": "text"
            }
          ]
        },
        {
          "type": "unorderedList",
          "items": [
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Display rich web content using HTML, CSS, and JavaScript",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "text": "Handle the incremental loading of page content",
                      "type": "text"
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "type": "paragraph",
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Display multiple MIME types and compound frame elements"
                    }
                  ]
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "text": "Navigate between pages of content",
                      "type": "text"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            },
            {
              "content": [
                {
                  "inlineContent": [
                    {
                      "type": "text",
                      "text": "Manage a forward-back list of recently visited pages"
                    }
                  ],
                  "type": "paragraph"
                }
              ]
            }
          ]
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "text": "For more information about WebKit, go to ",
              "type": "text"
            },
            {
              "isActive": true,
              "type": "reference",
              "identifier": "https://webkit.org"
            },
            {
              "text": ".",
              "type": "text"
            }
          ]
        }
      ]
    }
